> setwd("C:/+STRAUCH+/+PAPER_WORK+/Opti-Tool/CoMOLA_basic/models/SAR")
> sink("C:/+STRAUCH+/+PAPER_WORK+/Opti-Tool/CoMOLA_basic/models/SAR/console.txt", append=FALSE)
> ##########################################################################################
> #
> #     ~ ~ ~ Simple SAR (Species Area relationship) Model - Anna Cord 20.04.2015 ~ ~ ~
> #
> #     ~ ~ ~ Input data ~ ~ ~
> #    land_use.asc        |land use map containing the following classes
> #                        |1,2,3,4,5 = arable land with increasing intensity from 1 to 5
> #                        |6 = forest
> #                        |7 = pasture
> #                        |8 = urban area
> #                        |-2 = no data
> #    Objective: Maximize species richness
> #
> ##########################################################################################
> 
> # set working directory
> 
> # read in ascii files
> lu.map <- read.table("map.asc", h=F, skip=6, sep=" ")
> 
> # determine area (= number of cells) covered by forest
> forest.area <- length(which(lu.map == 7))          
>                     
> # calculate species richness based on species-area relationship (formula: S=c*A^z)
> c <- 5
> z <- 0.2
> 
> S <- c * forest.area^z
> S[is.na(S)] <- 0
> 
> # # only for testing: plot logarithmic function
> # S.100 <- vector("numeric",100)
> # for (i in 1:100) {
> #   S.100[i] <- c * i^z
> # }
> # plot(S.100)
> 
> # write model output
> write.table(S, "SAR_output.csv",append=FALSE ,sep =";",col.names=FALSE ,row.names=FALSE)
